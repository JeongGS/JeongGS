
public class Narray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a;
		a = new int[10];
		a[0] = 8;
		a[1] = 2;
		System.out.println(a[1]);
		
		char[] ch=new char[] {'a', 'b', 'c','d'};
		int[] b = {3, 4, 5, 6};
		System.out.println(ch[2]);
		System.out.println(b[1]);
	}

}
