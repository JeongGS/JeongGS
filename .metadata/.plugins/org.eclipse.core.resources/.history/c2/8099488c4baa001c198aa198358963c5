
public class Narray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a;
		a = new int[10];
		a[0] = 8;
		a[1] = 2;
		System.out.println(a[1]);
		
		char[] ch={'a', 'b', 'c','d'};
		int[] b = {3, 4, 5, 6};
		System.out.println(ch[1]);
		System.out.println(b[1]);

	int[] Otarray;
	Otarray = new int[5];
	Otarray[0] = 23;
	Otarray[1] = 32;
	Otarray[2] = 11;
	Otarray[3] = 22;
	Otarray[4] = 33;
	System.out.println(Otarray[3]);
	Otarray[] = [44, 55, 66, 77, 88];
	System.out.println(Otarray[3]);
	}
}
